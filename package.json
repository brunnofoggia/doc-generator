{
    "name": "docs-handler",
    "version": "0.15.0",
    "description": "",
    "license": "ISC",
    "author": "Bruno Foggia",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "scripts": {
        "husky:install": "husky install",
        "exec:kms": "IS_TS_NODE=true ts-node -r tsconfig-paths/register test/kms",
        "prebuild": "rimraf lib",
        "build": "tsc -p ./tsconfig.build.json",
        "lint": "eslint \"{src,apps,libs}/**/*.+(js|json|ts|tsx)\" --fix",
        "pre-commit": "lint-staged",
        "prepare": "npm run build",
        "test": "npm run test:docker:up && npm run test:run",
        "test:run": "NODE_OPTIONS=--experimental-vm-modules npx jest --verbose --detectOpenHandles",
        "test:docker:up": "docker compose up -d",
        "test:docker:down": "docker compose down"
    },
    "dependencies": {
        "cloud-solutions": "github:brunnofoggia/cloud-solutions",
        "debug": "^4.3.4",
        "ejs": "^3.1.9",
        "lodash": "^4.17.21"
    },
    "devDependencies": {
        "@commitlint/config-conventional": "^18.6.0",
        "@faker-js/faker": "^8.4.0",
        "@types/jest": "^29.5.12",
        "@types/lodash": "^4.14.202",
        "@types/node": "^20.11.16",
        "@typescript-eslint/eslint-plugin": "^6.20.0",
        "@typescript-eslint/parser": "^6.20.0",
        "axios": "^1.6.7",
        "commitlint": "^18.6.0",
        "cz-conventional-changelog": "^3.3.0",
        "dotenv": "^16.4.1",
        "eslint": "^8.56.0",
        "git-commit-msg-linter": "^5.0.6",
        "husky": "^9.0.10",
        "jest": "^29.7.0",
        "lint-staged": "^15.2.1",
        "node-fpdf": "^1.0.45",
        "node-labs": "github:brunnofoggia/node-labs",
        "pdfkit": "^0.14.0",
        "pg": "^8.11.3",
        "puppeteer": "^21.11.0",
        "rimraf": "^5.0.5",
        "ts-jest": "^29.1.2",
        "ts-loader": "^9.5.1",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typeorm": "^0.3.20",
        "typescript": "^5.3.3"
    },
    "files": [
        "/lib"
    ],
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}